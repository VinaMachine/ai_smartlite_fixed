{
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "type": "object",
    "required": [
        "event",
        "seq",
        "timestamp"
    ],
    "properties": {
        "event": {
            "type": "string"
        },
        "seq": {
            "type": "integer"
        },
        "timestamp": {
            "type": "string",
            "format": "date-time"
        },
        "payload": {
            "type": "object"
        }
    }
}